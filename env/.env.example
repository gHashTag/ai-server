# Railway Environment Variables Template

# App Configuration
NODE_ENV=production
APP_ENV=staging
PORT=${{ PORT }}

# Database & Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Security & Authentication
SECRET_KEY=your-secret-key-here
SECRET_API_KEY=your-secret-api-key-here
JWT_SECRET_KEY=your-jwt-secret-key

# External APIs
OPENAI_API_KEY=
REPLICATE_API_TOKEN=
ELEVENLABS_API_KEY=
BFL_API_KEY=
SYNC_LABS_API_KEY=


# Instagram Scraping (Apify) - используем только Apify  
APIFY_TOKEN=your-apify-token-here

# Management
NEXT_PUBLIC_MANAGEMENT_TOKEN=

# Bot Tokens - Production Bots (1-10)
BOT_TOKEN_1=
BOT_TOKEN_2=
BOT_TOKEN_3=
BOT_TOKEN_4=
BOT_TOKEN_5=
BOT_TOKEN_6=
BOT_TOKEN_7=
BOT_TOKEN_8=
BOT_TOKEN_9=
BOT_TOKEN_10=

# Bot Tokens - Test Bots
BOT_TOKEN_TEST_1=
BOT_TOKEN_TEST_2=

# Service Configuration
BASE_URL=https://your-app.railway.app
ORIGIN=https://your-app.railway.app
API_URL=https://your-app.railway.app

# Payment (Robokassa)
MERCHANT_LOGIN=
PASSWORD2=
RESULT_URL2=https://your-app.railway.app/payment/result

# Inngest
INNGEST_EVENT_KEY=
INNGEST_SIGNING_KEY=
INNGEST_WEBHOOK_URL=https://your-app.railway.app/api/inngest

# Rendering (Nexrender)
NEXRENDER_PORT=4001
NEXRENDER_SECRET=myapisecret
AERENDER_PATH=/usr/local/bin/aerender

# Monitoring & Logging
LOG_LEVEL=info
LOG_FORMAT=combined
LOG_DIR=/app/logs
ENABLE_LOGGING=true
CREDENTIALS=true

# Development (only for staging)
NGROK_URL=https://your-ngrok-url.ngrok.io