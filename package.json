{
  "name": "ai-server-express",
  "version": "0.0.0",
  "description": "TypeScript + Express API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "start:build": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "node scripts/dev-full.js",
    "dev:server": "npm run build && cross-env NODE_ENV=development node dist/server.js",
    "dev:inngest": "npx inngest-cli@latest dev -u http://localhost:4000/api/inngest --port 8289 --log-level warn",
    "dev:full": "node scripts/dev-full.js",
    "dev:mcp": "npm run build && node dist/mcp/index.js",
    "logs:clean": "node scripts/logs-viewer.js important",
    "logs:errors": "node scripts/logs-viewer.js error",
    "logs:api": "node scripts/logs-viewer.js api",
    "logs:minimal": "node scripts/logs-viewer.js minimal",
    "logs:help": "node scripts/logs-viewer.js --help",
    "build": "npx swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src",
    "lint:fix": "npm run lint -- --fix",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "dev:worker": "NODE_OPTIONS='--no-warnings' npx @nexrender/worker --binary=\"/Applications/Adobe After Effects 2025/aerender\" --host=http://localhost:4001 --secret=myapisecret",
    "format": "prettier --write .",
    "ngrok": "ngrok http 2999",
    "tunnel": "ssh -R 80:localhost:4000 localhost.run",
    "mcp:server": "npm run build && node dist/mcp/index.js",
    "mcp:server:direct": "node dist/mcp/index.js",
    "mcp:dev": "nodemon --exec \"npm run build && node dist/mcp/index.js\" --watch src/mcp --ext ts",
    "n8n:test": "node tests/n8n/test-n8n-integration.js",
    "dev:n8n": "N8N_BASIC_AUTH_ACTIVE=true N8N_BASIC_AUTH_USER=admin N8N_BASIC_AUTH_PASSWORD=admin123 N8N_HOST=localhost N8N_PORT=5678 N8N_PROTOCOL=http N8N_SECURE_COOKIE=false npx n8n start",
    "n8n:standalone": "N8N_BASIC_AUTH_ACTIVE=true N8N_BASIC_AUTH_USER=admin N8N_BASIC_AUTH_PASSWORD=admin123 N8N_HOST=localhost N8N_PORT=5678 N8N_PROTOCOL=http N8N_SECURE_COOKIE=false npx n8n start",
    "test:ecosystem": "./scripts/test-dev-ecosystem.sh"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
