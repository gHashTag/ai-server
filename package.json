    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.5.1",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.1",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "inngest": "^3.37.0",
    "jest-mock": "^29.7.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "micro": "^10.0.1",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "openai": "^4.77.0",
    "pg": "^8.16.2",
    "reflect-metadata": "^0.1.14",
    "replicate": "^0.32.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.5.0",
    "telegraf": "^4.16.3",
    "tslib": "^2.8.1",
    "typedi": "^0.10.0",
    "uuid": "^11.0.3",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5",
