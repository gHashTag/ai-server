    "@supabase/supabase-js": "^2.47.10",
    "apify-client": "^2.15.0",
    "@types/archiver": "^6.0.3",
    "@types/multer": "^1.4.12",
    "@types/pg": "^8.15.4",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "axios": "^1.7.9",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
