# AI Server Environment Variables Example

# Execution Plans Configuration
USE_INNGEST=true              # Enable/disable Inngest (Plan A)
FALLBACK_MODE=false           # Force fallback mode (Plan B)

# Inngest Configuration
INNGEST_EVENT_KEY=your_inngest_event_key
INNGEST_SIGNING_KEY=your_inngest_signing_key
INNGEST_WEBHOOK_URL=https://yourdomain.com/api/inngest

# Database
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_KEY=your_supabase_service_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# API Keys
SECRET_KEY=your_secret_key
SECRET_API_KEY=your_secret_api_key
ELEVENLABS_API_KEY=your_elevenlabs_api_key
REPLICATE_API_TOKEN=your_replicate_token
SYNC_LABS_API_KEY=your_sync_labs_key

# Bot Configuration
BOT_TOKEN_1=your_bot_token_1
BOT_TOKEN_2=your_bot_token_2
BOT_TOKEN_3=your_bot_token_3
BOT_TOKEN_4=your_bot_token_4
BOT_TOKEN_TEST_1=your_test_bot_token_1
BOT_TOKEN_TEST_2=your_test_bot_token_2

# Server Configuration
NODE_ENV=development
PORT=3000
ORIGIN=https://yourdomain.com
NGROK_URL=https://your-ngrok-url.ngrok.io

# Additional Services
NEXRENDER_PORT=3050
NEXRENDER_SECRET=your_nexrender_secret
AERENDER_PATH=/path/to/aerender

# Payment Configuration
MERCHANT_LOGIN=your_merchant_login
PASSWORD2=your_payment_password
RESULT_URL2=your_result_url
NEXT_PUBLIC_MANAGEMENT_TOKEN=your_management_token

# Logging
LOG_FORMAT=combined
LOG_DIR=logs

# Other
CREDENTIALS=true